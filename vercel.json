{
    "version": 2,
    "builds": [
        {
            "src": "api/[...path].js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/[...path].js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}